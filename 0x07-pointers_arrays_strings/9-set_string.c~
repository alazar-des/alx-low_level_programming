#include <stdio.h>

/**
 * set_string - set string value
 * @s: pointer to string pointer
 * @to: pointer string to set value
 *
 */
void set_string(char **s, char *to)
{
  unsigned long int i;
  char *p;
  char c = 'a';
  
  p = *s;
  i = 0;
  while (to[i] != '\0')
    {
      p[i] = c;
      printf("%c", c);
      i++;
    }
}
